[
    {
        "request": {
            "method": "GET",
            "url": "https://www.hddolby.com/attendance.php",
            "headers": [
                {
                    "name": "user-agent",
                    "value": "Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/89.0.4389.90 Safari/537.36 Edg/89.0.774.63"
                },
                {
                    "name": "Host",
                    "value": "www.hddolby.com"
                },
				{
                    "name": "Connection",
                    "value": "keep-alive"
                },
                {
                    "name": "upgrade-insecure-requests",
                    "value": "1"
                },
                {
                    "name": "cache-control",
                    "value": "max-age=0"
                },
                {
                    "name": "cookie",
                    "value": "{{ cookie }}"
                }
            ],
            "cookies": []
        },
        "rule": {
            "success_asserts": [
				{
					"re":"200",
					"from":"status"
				},
				{
					"re": "签到成功",
					"from": "content"
				},
                {
                    "re": "已经签到",
                    "from": "content"
                }
            ],
            "failed_asserts": [],
			"extract_variables": [
				{
					"re": "<td class="text">\s*(.*?)<\/td>",
					"from": "content",
					"name": "__log__"
				}
			]
        }
    }
]
